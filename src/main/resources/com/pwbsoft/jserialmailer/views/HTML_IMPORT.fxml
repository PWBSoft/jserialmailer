                prefWidth="85.0" text="Select HTML" AnchorPane.leftAnchor="380.0" AnchorPane.topAnchor="36.0"/>
        <WebView fx:id="webView" layoutX="15.0" layoutY="77.0" prefHeight="172.0" prefWidth="451.0"
                 AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0"
                 AnchorPane.topAnchor="75.0"/>
        <Button fx:id="nextButton" disable="true" layoutX="413.0" layoutY="259.0" mnemonicParsing="false" text="Next"
                AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0"/>
    </children>
</AnchorPane>
