<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefWidth="500" prefHeight="500" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="csvFileLabel" editable="false" layoutX="14.0" layoutY="36.0" prefHeight="25.0" prefWidth="356.0" promptText="Select CSV File" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="36.0" />
      <Button fx:id="csvFileButton" layoutX="380.0" layoutY="36.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" text="Select CSV" AnchorPane.leftAnchor="380.0" AnchorPane.topAnchor="36.0" />
      <ScrollPane layoutX="15.0" layoutY="77.0" prefHeight="172.0" prefWidth="451.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="75.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TableView fx:id="table" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="email" />
                      <TableColumn prefWidth="75.0" text="name" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <Button fx:id="nextButton" disable="true" layoutX="413.0" layoutY="259.0" mnemonicParsing="false" text="Next" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" />
   </children>
</AnchorPane>
